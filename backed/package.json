{
  "name": "node-typescript-koa-rest",
  "version": "1.5.0",
  "description": "API REST using NodeJS and KOA framework, typescript. TypeORM for SQL with class-validators. Middlewares JWT, CORS, Winston Logger. Swagger API docs",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --watch src -e ts,tsx --exec ts-node -r tsconfig-paths/register src/app.ts",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "lint": "tslint -c tslint.json --fix -p tsconfig.json",
    "serve": "node dist/app.js",
    "build": "tsc",
    "start": "npm run serve",
    "test": "jest"
  },
  "engines": {
    "npm": "6.x.x"
  },
  "author": "Javier Aviles",
  "license": "MIT",
  "homepage": "https://github.com/javieraviles/node-typescript-koa-rest#readme",
  "keywords": [
    "typescript",
    "koa",
    "koa2",
    "jwt",
    "winston",
    "rest",
    "starter",
    "typeORM",
    "class-validator",
    "helmet",
    "sql",
    "api",
    "rest",
    "heroku",
    "cors"
  ],
  "repository": "github:javieraviles/node-typescript-koa-rest",
  "devDependencies": {
    "@koa/cors": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.18",
    "@types/koa": "2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa__cors": "^2.2.3",
    "@types/mongoose": "^5.5.12",
    "@types/node": "^12.0.12",
    "@types/shelljs": "^0.8.0",
    "@types/socket.io": "^2.1.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.8.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@types/koa-static": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "dotenv": "^8.0.0",
    "google-auth-library": "^5.7.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.2.0",
    "koa-jwt": "^3.3.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-swagger-decorator": "^1.5.16",
    "moment": "^2.24.0",
    "mongoose": "^5.6.8",
    "pg": "^7.4.3",
    "pg-connection-string": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.2.0",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "xlsx": "^0.15.2"
  }
}
